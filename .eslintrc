{
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "optionalChaining": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "no-duplicate-imports": "error"
    }
}
